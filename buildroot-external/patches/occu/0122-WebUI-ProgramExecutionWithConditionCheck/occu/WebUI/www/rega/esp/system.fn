  WriteLine('setDate("' # sDate # '");');
!  Write( 'if( $("maintime") ) $("maintime").innerHTML = "'#sTime#'";' );
!  Write( 'if( $("maindate") ) $("maindate").innerHTML = "'#sDate#'";' );
  
  object oPresence = dom.GetObject( ID_PRESENT );
