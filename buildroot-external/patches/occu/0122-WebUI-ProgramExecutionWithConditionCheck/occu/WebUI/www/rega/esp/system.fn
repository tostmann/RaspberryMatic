    {
      i1 = i1 + 1;
      integer i2 = 0;
      string s2;
      foreach( s2, system.GetVar("timestamps") )
      {
        i2 = i2 + 1;
        if( i2 == i1 )
        {
          sTM = s2;
        } 
      }
      integer cId = s1;
      object oTmp = dom.GetObject( cId );
      if( oTmp )
      {
        string sLastTime = "";
